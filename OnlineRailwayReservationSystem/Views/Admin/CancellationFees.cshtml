  <!-- Cancellation Fees Master Start -->

  @model List<CancellationFees>

<div class="container-fluid pt-4 px-4">
    <div class="row g-4">
        <div class="col-sm-12 col-xl-12">
            <div class="bg-light text-center rounded p-4">
                <div class="d-flex align-items-center justify-content-center mb-4">
                    <h5 class="mb-0">Cancellation Fees Rule Master</h5>
                </div>
                <table class="table table-striped table-info">
                    <thead>
                        <tr>
                            <th>Can. ID</th>
                            <th>Before 24 Hours (Rs)</th>
                            <th>Before 12 Hours (Rs)</th>
                            <th>Before 1 Hours (Rs)</th>
                            <th>Edit</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var data in Model)
                        {
                            <tr>
                                <td>@data.CancellationFees_Id</td>
                                <td>@data.Cancellation_24Hours</td>
                                <td>@data.Cancellation_12Hours</td>
                                <td>@data.Cancellation_1Hours</td>
                                <td>
                                    <a href="@Url.Action("CancellationFeesEdit", "Admin", new{id=data.CancellationFees_Id})" class="btn btn-primary btn-sm">Edit</a>
                                </td>
                            </tr>
                        }
                        
                    </tbody>
                </table>
                <div class="d-flex align-items-start">
                    <a href="@Url.Action("CancellationFeesAdd", "Admin")">
                        <button class="btn btn-primary">
                            Add New
                        </button>
                    </a>
                    <button class="ms-2 btn btn-primary">
                        Back
                    </button>
                </div>
            </div>
        </div>
    </div>
</div>

<!-- Cancellation Fees Master End -->