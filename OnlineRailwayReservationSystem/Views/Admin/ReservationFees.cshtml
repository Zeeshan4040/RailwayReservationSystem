<!-- Reservation Fees Master Start -->

@model List<ReservationFees>

<div class="container-fluid pt-4 px-4">
    <div class="row g-4">
        <div class="col-sm-12 col-xl-12">
            <div class="bg-light text-center rounded p-4">
                <div class="d-flex align-items-center justify-content-center mb-4">
                    <h5 class="mb-0">Reservation And Super Fast Fees Master</h5>
                </div>
                <table class="table table-striped table-info">
                    <thead>
                        <tr>
                            <th>Coach Type</th>
                            <th>Reservation Fees</th>
                            <th>Super Fast Fees</th>
                            <th>Edit</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var data in Model)
                        {
                            <tr>
                                <td>@data.Coach_Type</td>
                                <td>@data.Reservation_Fees</td>
                                <td>@data.Super_Fast_Fees</td>
                                <td>
                                    <a href="@Url.Action("ReservationFeesEdit", "Admin", new{id=data.ReservationFees_Id})" class="btn btn-primary btn-sm">Edit</a>
                                </td>
                            </tr>
                        }                        
                    </tbody>
                </table>
                <div class="d-flex align-items-start">
                    <a href="@Url.Action("ReservationFeesAdd", "Admin")">
                        <button class="btn btn-primary">
                            Add New
                        </button>
                    </a>
                    <button class="ms-2 btn btn-primary">
                        Back
                    </button>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Reservation Fees Master End -->